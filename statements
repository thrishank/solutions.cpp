#include <iostream>
#include <algorithm>
using namespace std;


int main() {
     int a;
     int b;
     int c;
     int d;
     
     cin>>a>>b>>c>>d;
     if (a>b && b>c && c>d) {
         cout<<a;
     }
     else if (a<b && b>c && c>d){
         cout<<b;
     }
     else if (a<b && b<c && c>d) {
         cout<<c;
     }
     else {
         cout<<d;
     }
     return 0;
}
